{
    "env": {
        "node": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "no-console": 0,
        "indent": [
            "error",
            4
        ],
        "import/newline-after-import": [
            "error",
            {
                "count": 1
            }
        ]
    }
}